"~/bin/mute_toggle"
  m:0x0 + c:121
  XF86AudioMute

"/usr/bin/amixer sset Master 5%-"
  m:0x0 + c:122
  XF86AudioLowerVolume

"/usr/bin/amixer sset Master 5%+"
  m:0x0 + c:123
  XF86AudioRaiseVolume

"/usr/bin/xbacklight -dec 10"
  m:0x0 + c:232
  XF86MonBrightnessDown

"/usr/bin/xbacklight -inc 10"
  m:0x0 + c:233
  XF86MonBrightnessUp
